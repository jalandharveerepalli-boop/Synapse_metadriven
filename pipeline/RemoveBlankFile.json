{
	"name": "RemoveBlankFile",
	"properties": {
		"activities": [
			{
				"name": "Remove File if Blank",
				"description": "Need to Factor in CDCType and Persist Staging Flag",
				"type": "IfCondition",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@equals(pipeline().parameters.RowsToADLS,'0')",
						"type": "Expression"
					},
					"ifTrueActivities": [
						{
							"name": "Delete ADLS If Blank",
							"type": "Delete",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"dataset": {
									"referenceName": "dsParquet_Azure",
									"type": "DatasetReference",
									"parameters": {
										"Container": {
											"value": "@pipeline().parameters.Container",
											"type": "Expression"
										},
										"Folder": {
											"value": "@pipeline().parameters.WorkingFilePathName",
											"type": "Expression"
										},
										"FileName": {
											"value": "@pipeline().parameters.WorkingFileName",
											"type": "Expression"
										},
										"storageuri": {
											"value": "@pipeline().parameters.StorageURI",
											"type": "Expression"
										}
									}
								},
								"enableLogging": false,
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"RowsToADLS": {
				"type": "string"
			},
			"Container": {
				"type": "string"
			},
			"WorkingFilePathName": {
				"type": "string"
			},
			"WorkingFileName": {
				"type": "string"
			},
			"StorageURI": {
				"type": "string"
			}
		},
		"folder": {
			"name": "MDPUtil"
		},
		"annotations": []
	}
}