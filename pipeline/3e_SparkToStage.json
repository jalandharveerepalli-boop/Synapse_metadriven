					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "LocalFileList",
						"type": "DatasetReference",
						"parameters": {
							"Directory": {
								"value": "@pipeline().parameters.Directory",
								"type": "Expression"
							},
							"storageuri": {
								"value": "@pipeline().parameters.storageuri",
								"type": "Expression"
							},
							"container": "synapse",
							"file": {
								"value": "@concat('','')",
								"type": "Expression"
							}
						}
					},
