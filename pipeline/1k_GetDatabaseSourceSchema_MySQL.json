															"value": "SET @SourceName = '@{item().SourceTable}';\n\nSET @SourceSchema = '@{item().SourceSchema}';\n\nSET @Domain = '@{item().Domain}';\n\nSET @Subject = '@{item().Subject}';\n\nSET @FeedKey = '@{int(item().FeedKey)}';\n\n-- Primary Keys\nSET @PKList = '';\n    \nSet @PKList = (Select PKlist from (\nSELECT DummyKey, group_concat(concat(COLUMN_NAME,' ASC')) as PKList\n    FROM (select 'a' as DummyKey, Column_name, Table_name, table_schema, column_key from INFORMATION_SCHEMA.Columns tc\n    WHERE tc.TABLE_NAME = @SourceName \n\t\tand tc.table_schema = @SourceSchema\n\t\tAND tc.Column_Key ='PRI'\n        UNION \n        select 'a' as DummyKey, Column_name, Table_name, table_schema, column_key from INFORMATION_SCHEMA.Columns tc\n    WHERE tc.TABLE_NAME = @SourceName \n\t\tand tc.table_schema = @SourceSchema\n\t\tAND tc.Column_Key ='UNI'\n        ) a\n\tGROUP BY DummyKey) a);\n\nSELECT \n    @FeedKey            As FeedKey, \n    @SourceName         AS TableName,\n    @SourceSchema       AS SchemaName,\n    ordinal_position    AS FieldPosition, \n    0                   AS ScriptHeader,\n    concat('[',COLUMN_NAME,'] ' ,\n        '[',\n        case when DATA_TYPE = 'datetime' then 'datetime2' \n\t\t\twhen Data_Type in ('mediumint', 'year', 'smallint', 'tinyint') then 'INT' \n            when data_type in ('float', 'double', 'real') then 'Float'\n            when data_type = 'timestamp' then 'bigint'\n            when data_type in ('char', 'varchar', 'tinytext', 'text', 'mediumtext', 'longtext') then 'Nvarchar'\n            when data_type in ('varbinary', 'tinyblob', 'blob', 'mediumblob', 'longblob') then 'Varbinary'\n\t\t\telse DATA_TYPE end ,\n        ']',\n        case \n            when DATA_TYPE in ('char', 'varchar', 'tinytext', 'text', 'mediumtext', 'longtext', 'nchar') then concat('(',replace(convert(CHARACTER_MAXIMUM_LENGTH, NCHAR),'-1','MAX'),')')\n            when DATA_TYPE in ('varbinary', 'tinyblob', 'blob', 'mediumblob', 'longblob') then concat('(',replace(convert(CHARACTER_MAXIMUM_LENGTH, NCHAR),'-1','MAX'),')')\n            when DATA_TYPE in ('timestamp', 'int', 'bigint', 'uniqueidentifier', 'mediumint', 'year', 'float', 'double', 'real') then ''\n            when DATA_TYPE in ('datetime', 'datetime2') then '(0)'\n            when DATA_TYPE in ('numeric') then concat('(',convert(NUMERIC_PRECISION, NCHAR),',',convert(NUMERIC_SCALE, NCHAR),')')\n            else '' \n        end ,\n        case when Column_Key = 'UNI' or Column_Key = 'PRI' then ' NOT NULL' else ' NULL' end ,\n        case \n            when ORDINAL_POSITION= (select max(ordinal_Position) from INFORMATION_SCHEMA.COLUMNS where TABLE_NAME = @SourceName) \n            then concat(', CONSTRAINT [PK_',@SourceName,'] PRIMARY KEY CLUSTERED ( ',@PKList,') )')\n            else ',' end\n            )\t\t\t                                        as FieldCreateSQL,\n\n    COLUMN_NAME                                                 as FieldName,\n    COLUMN_NAME                                                 as FieldWarehouseName, -- for WarehouseMapping\n\n    concat('[',\n        case when DATA_TYPE = 'datetime' then 'datetime2' else DATA_TYPE end ,\n        ']' ,\n        case \n            when DATA_TYPE in ('char', 'varchar', 'tinytext', 'text', 'mediumtext', 'longtext', 'nchar') then concat('(',replace(convert(CHARACTER_MAXIMUM_LENGTH, nchar),'-1','MAX'),')')\n            when DATA_TYPE in ('varbinary', 'tinyblob', 'blob', 'mediumblob', 'longblob') then concat('(',replace(convert(CHARACTER_MAXIMUM_LENGTH, nchar),'-1','MAX'),')')\n            when DATA_TYPE in ('timestamp', 'int', 'bigint', 'uniqueidentifier', 'mediumint', 'year', 'float', 'double', 'real') then ''\n            when DATA_TYPE in ('datetime', 'datetime2') then '(0)'\n            when DATA_TYPE in ('numeric') then concat('(',convert(NUMERIC_PRECISION, nchar),',',convert(NUMERIC_SCALE, nchar),')')\n            else '' \n        end\n        )\t\t\t\t\t\t\t\t\t\t\t\t\t\tas FieldDataType,\n    case when column_Key = 'UNI' or Column_key = 'PRI' then 1 else 0 end                    as PrimaryKeyCol -- PrimaryKeyField \n\nFROM INFORMATION_SCHEMA.COLUMNS c\nWHERE \n    TABLE_NAME = @SourceName\nAND \n    TABLE_SCHEMA = @SourceSchema\nORDER BY \n    SchemaName, \n    FieldPosition",
