ey, group_concat(concat(COLUMN_NAME,' ASC')) as PKList\n\t\t\t\t\t\t\t\tFROM (select 'a' as DummyKey, Column_name, Table_name, table_schema, column_key from INFORMATION_SCHEMA.Columns tc\n\t\t\t\t\t\t\t\tWHERE tc.TABLE_NAME = '@{item().SourceTable}'\n\t\t\t\t\t\t\t\t\tand tc.table_schema = '@{item().SourceSchema}'\n\t\t\t\t\t\t\t\t\tAND tc.Column_Key ='PRI'\n\t\t\t\t\t\t\t\t\tUNION \n\t\t\t\t\t\t\t\t\tselect 'a' as DummyKey, Column_name, Table_name, table_schema, column_key from INFORMATION_SCHEMA.Columns tc\n\t\t\t\t\t\t\t\tWHERE tc.TABLE_NAME = '@{item().SourceTable}'\n\t\t\t\t\t\t\t\t\tand tc.table_schema = '@{item().SourceSchema}'\n\t\t\t\t\t\t\t\t\tAND tc.Column_Key ='UNI'\n\t\t\t\t\t\t\t\t\t) a\n\t\t\t\t\t\t\t\tGROUP BY DummyKey) a),') )')\n            else ',' end\n            )\t\t\t                                        as FieldCreateSQL,\n\n    COLUMN_NAME                                                 as FieldName,\n    COLUMN_NAME                                                 as FieldWarehouseName, \n\n    concat('[',\n        case when DATA_TYPE = 'datetime' then 'datetime2' else DATA_TYPE end ,\n        ']' ,\n        case \n            when DATA_TYPE in ('char', 'varchar', 'tinytext', 'text', 'mediumtext', 'longtext', 'nchar') then concat('(',replace(convert(CHARACTER_MAXIMUM_LENGTH, nchar),'-1','MAX'),')')\n            when DATA_TYPE in ('varbinary', 'tinyblob', 'blob', 'mediumblob', 'longblob') then concat('(',replace(convert(CHARACTER_MAXIMUM_LENGTH, nchar),'-1','MAX'),')')\n            when DATA_TYPE in ('timestamp', 'int', 'bigint', 'uniqueidentifier', 'mediumint', 'year', 'float', 'double', 'real') then ''\n            when DATA_TYPE in ('datetime', 'datetime2') then '(0)'\n            when DATA_TYPE in ('numeric') then concat('(',convert(NUMERIC_PRECISION, nchar),',',convert(NUMERIC_SCALE, nchar),')')\n            else '' \n        end\n        )\t\t\t\t\t\t\t\t\t\t\t\t\t\tas FieldDataType,\n    case when column_Key = 'UNI' or Column_key = 