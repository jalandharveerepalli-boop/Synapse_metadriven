{
	"name": "dsArchiveFile",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lsADLS",
			"type": "LinkedServiceReference",
			"parameters": {
				"storageurl": {
					"value": "@dataset().storageuri",
					"type": "Expression"
				}
			}
		},
		"parameters": {
			"storageuri": {
				"type": "string",
				"defaultValue": "https://hapdatastorageactdev.dfs.core.windows.net"
			}
		},
		"folder": {
			"name": "CSV"
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(string(utcnow()),item().name)",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@formatDateTime(trigger().startTime,'dd-MM-yyyy')",
					"type": "Expression"
				},
				"fileSystem": "archive"
			}
		}
	},
	"type": "Microsoft.Synapse/workspaces/datasets"
}