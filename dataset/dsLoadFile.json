{
	"name": "dsLoadFile",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lsADLS_Azure",
			"type": "LinkedServiceReference",
			"parameters": {
				"storageurl": {
					"value": "@dataset().storageuri",
					"type": "Expression"
				}
			}
		},
		"parameters": {
			"storageuri": {
				"type": "string",
				"defaultValue": "https://hapdatastorageactdev.dfs.core.windows.netc"
			},
			"container": {
				"type": "string",
				"defaultValue": "import"
			},
			"directory": {
				"type": "string"
			},
			"file": {
				"type": "string",
				"defaultValue": "@item().name"
			}
		},
		"folder": {
			"name": "CSV"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().file",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().directory",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.Synapse/workspaces/datasets"
}