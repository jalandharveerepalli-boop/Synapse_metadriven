				"KeySecretName": {
					"value": "@dataset().S3KeySecretName",
					"type": "Expression"
				},
				"keyvaultdnsname": {
					"value": "@dataset().keyvaultdnsname",
					"type": "Expression"
				}
			}
		},
		"parameters": {
			"S3Bucket": {
				"type": "string",
				"defaultValue": "file-uploads-tst-sroc-service-gov-uk"
			},
			"S3Directory": {
				"type": "string",
				"defaultValue": "csv"
			},
			"S3FileName": {
				"type": "string",
				"defaultValue": "transactions.csv"
			},
			"S3AccessKey": {
				"type": "string",
				"defaultValue": "AKIAZLGZKNL7ESGHPV2P"
			},
			"S3KeySecretName": {
				"type": "string",
				"defaultValue": "AmazonS3KeySecret"
			},
			"keyvaultdnsname": {
				"type": "string",
				"defaultValue": "https://sndrsdkv1401.vault.azure.net/"
